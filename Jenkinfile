pipeline {
    agent any
    
	stages {
        stage('Checkout') {
           steps { 
				git url: 'https://github.com/vinitagarg02/CaseStudy.git', credentialsId: 'github', branch: 'master'
		   }
        }

        stage('Build') {
			steps {
				
				bat 'mvn clean install'
				
				}
			}       
		}
	}